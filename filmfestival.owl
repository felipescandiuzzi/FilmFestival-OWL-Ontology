<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.thodoris.net/ontology/film-festival-model/"
     xml:base="http://www.thodoris.net/ontology/film-festival-model/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:ns="http://creativecommons.org/ns#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#"
     xmlns:t="http://purl.org/NET/c4dm/timeline.owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:ffschema="http://www.thodoris.net/ontology/film-festival-model/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:event="http://purl.org/NET/c4dm/event.owl#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:ontology="http://dbpedia.org/ontology/"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:dc="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://www.thodoris.net/ontology/film-festival-model/">
        <owl:imports rdf:resource="http://dbpedia.org/ontology/"/>
        <rdfs:comment>A Film Festival OWL ontology</rdfs:comment>
        <owl:priorVersion rdf:resource="http://www.thodoris.net/ontology/film-festival-model/old"/>
        <rdfs:label>Film Festival Ontology</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    

    <!-- http://purl.org/NET/c4dm/event.owl#place -->

    <owl:AnnotationProperty rdf:about="http://purl.org/NET/c4dm/event.owl#place"/>
    


    <!-- http://purl.org/NET/c4dm/event.owl#time -->

    <owl:AnnotationProperty rdf:about="http://purl.org/NET/c4dm/event.owl#time"/>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#end -->

    <owl:AnnotationProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#end"/>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#start -->

    <owl:AnnotationProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#start"/>
    


    <!-- http://purl.org/dc/terms/source -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/source"/>
    


    <!-- http://www.thodoris.net/ontology/film-festival-model/Movie -->

    <owl:AnnotationProperty rdf:about="Movie"/>
    



    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#lat -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#lat"/>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#long -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#long"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#closeMatch -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#closeMatch"/>
    


    <!-- http://xmlns.com/foaf/0.1/based_near -->

    <owl:AnnotationProperty rdf:about="http://xmlns.com/foaf/0.1/based_near"/>
    


    <!-- http://xmlns.com/foaf/0.1/homepage -->

    <owl:AnnotationProperty rdf:about="http://xmlns.com/foaf/0.1/homepage"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    
     <!-- http://www.thodoris.net/ontology/film-festival-model/servesCuisine -->

		<owl:ObjectProperty rdf:about="servesCuisine">
			<rdfs:domain rdf:resource="Restaurant"/>
			<rdfs:range rdf:resource="Cusine"/>
			<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">servesCuisine</rdfs:label>
		 </owl:ObjectProperty>
	 
	    
    <!-- http://www.thodoris.net/ontology/film-festival-model/priceRange -->

    <owl:ObjectProperty  rdf:about="priceRange">
        <rdfs:domain rdf:resource="LocalBusiness"/>
        <rdfs:range rdf:resource="PriceRange"/>
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">priceRange</rdfs:label>
    </owl:ObjectProperty>

	<!-- http://www.thodoris.net/ontology/film-festival-model/hasGenre -->

    <owl:ObjectProperty rdf:about="hasGenre">
        <rdfs:domain rdf:resource="Movie"/>
		<rdfs:range rdf:resource="Genre"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Genre of a Movie</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasGenre</rdfs:label>
    </owl:ObjectProperty>
	
    

    <!-- http://www.thodoris.net/ontology/film-festival-model/hasDirector -->

    <owl:ObjectProperty rdf:about="hasDirector">
		<owl:equivalentProperty rdf:resource="isDirectedBy" />
        <rdfs:domain rdf:resource="Movie"/>
        <rdfs:range rdf:resource="Director"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A director of a movie</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasDirector</rdfs:label>
    </owl:ObjectProperty>
    
	
    <owl:ObjectProperty rdf:about="isDirectedBy">
        <rdfs:domain rdf:resource="Movie"/>
        <rdfs:range rdf:resource="Director"/>
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">isDirectedBy</rdfs:label>
    </owl:ObjectProperty>

    <!-- http://www.thodoris.net/ontology/film-festival-model/hasEvent -->

    <owl:ObjectProperty rdf:about="hasEvent">
        <rdfs:domain rdf:resource="Festival"/>
        <rdfs:range rdf:resource="http://purl.org/NET/c4dm/event.owl#Event"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An event of a FilmFestival</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasEvent</rdfs:label>
    </owl:ObjectProperty>
    
	<!-- http://www.thodoris.net/ontology/film-festival-model/hasActor -->

    <owl:ObjectProperty rdf:about="hasActor">
        <owl:inverseOf rdf:resource="isActorIn"/>
        <rdfs:domain rdf:resource="Movie"/>
        <rdfs:range rdf:resource="Actor"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Actors participating in a movie</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasActor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.thodoris.net/ontology/film-festival-model/hasActress -->

    <owl:ObjectProperty rdf:about="hasActress">
        <rdfs:subPropertyOf rdf:resource="hasActor"/>
        <owl:inverseOf rdf:resource="isActressIn"/>
        <rdfs:domain rdf:resource="Movie"/>
		<rdfs:range rdf:resource="WomenActors"/>
        <owl:propertyDisjointWith rdf:resource="hasMaleActor"/>
		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Actress participating in a movie</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasActress</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.thodoris.net/ontology/film-festival-model/hasMaleActor -->

    <owl:ObjectProperty rdf:about="hasMaleActor">
        <rdfs:subPropertyOf rdf:resource="hasActor"/>
        <owl:inverseOf rdf:resource="isMaleActorIn"/>
        <rdfs:domain rdf:resource="Movie"/>
		<rdfs:range rdf:resource="MenActors"/>
		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Men actors participating in a movie</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasMaleActor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.thodoris.net/ontology/film-festival-model/isActorIn -->

    <owl:ObjectProperty rdf:about="isActorIn">
        <rdfs:domain rdf:resource="Actor"/>
        <rdfs:range rdf:resource="Movie"/>
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">isActorIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.thodoris.net/ontology/film-festival-model/isActressIn -->

    <owl:ObjectProperty rdf:about="isActressIn">
        <rdfs:subPropertyOf rdf:resource="isActorIn"/>
        <owl:propertyDisjointWith rdf:resource="isMaleActorIn"/>
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">isActressIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.thodoris.net/ontology/film-festival-model/isMaleActorIn -->

    <owl:ObjectProperty rdf:about="isMaleActorIn">
        <rdfs:subPropertyOf rdf:resource="isActorIn"/>
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">isMaleActorIn</rdfs:label>
    </owl:ObjectProperty>
    
	

    <!-- http://www.thodoris.net/ontology/film-festival-model/sex -->

    <owl:ObjectProperty rdf:about="sex">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="Person"/>
        <rdfs:range rdf:resource="PersonSex"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sex of a person.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sex</rdfs:label>
    </owl:ObjectProperty>
	
   <!-- http://www.thodoris.net/ontology/film-festival-model/adjacentPlace -->
   <owl:ObjectProperty rdf:ID="adjacentPlace">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty" />
    <rdfs:domain rdf:resource="Place" />
    <rdfs:range rdf:resource="Place" />
  </owl:ObjectProperty>

	
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // DatatypeProperties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

	
    <!-- http://www.thodoris.net/ontology/film-festival-model/name -->

    <owl:DatatypeProperty rdf:about="name">
         <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
		 <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">name</rdfs:label>
    </owl:DatatypeProperty>
	
	
	<!-- http://www.thodoris.net/ontology/film-festival-model/description -->

   <owl:DatatypeProperty  rdf:about="description">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
		 <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">description</rdfs:label>
   </owl:DatatypeProperty>
	
	
	<owl:DatatypeProperty rdf:about="firstname">
		  <rdfs:domain rdf:resource="Person"/>
          <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
		  <rdfs:subPropertyOf rdf:resource="name"/>
		  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">First name</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">firstname</rdfs:label>
    </owl:DatatypeProperty>

    <!-- http://www.thodoris.net/ontology/film-festival-model/lastName -->

    <owl:DatatypeProperty rdf:about="lastname">
        <rdfs:domain rdf:resource="Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
		 <rdfs:subPropertyOf rdf:resource="name"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Last name</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">lastname</rdfs:label>
    </owl:DatatypeProperty>
    
	  <!-- http://www.thodoris.net/ontology/film-festival-model/birthDate -->

    <owl:DatatypeProperty rdf:about="birthDate">
        <rdfs:domain rdf:resource="Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">birthDate</rdfs:label>
    </owl:DatatypeProperty>

    <!-- http://www.thodoris.net/ontology/film-festival-model/maximumAttendeeCapacity -->

    <owl:DatatypeProperty rdf:about="maximumAttendeeCapacity">
        <rdfs:domain rdf:resource="Place"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">maximumAttendeeCapacity</rdfs:label>
    </owl:DatatypeProperty>
    
	 <!-- http://www.thodoris.net/ontology/film-festival-model/academyAwards -->

   <owl:DatatypeProperty rdf:about="academyAwards">
        <rdfs:domain rdf:resource="Director"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">academyAwards</rdfs:label>
    </owl:DatatypeProperty>
    
	<!-- http://www.thodoris.net/ontology/film-festival-model/oscarAwards -->

   <owl:DatatypeProperty rdf:about="oscarAwards">
        <rdfs:domain rdf:resource="Actor"/>
         <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
		 <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">oscarAwards</rdfs:label>
    </owl:DatatypeProperty>
    
	<!-- http://www.thodoris.net/ontology/film-festival-model/smokingAllowed -->

     <owl:DatatypeProperty rdf:about="smokingAllowed">
        <rdfs:domain rdf:resource="Place"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">smokingAllowed</rdfs:label>
    </owl:DatatypeProperty>
    
	 <!-- http://www.thodoris.net/ontology/film-festival-model/isAlive -->

    <owl:DatatypeProperty rdf:about="isAlive">
        <rdfs:domain rdf:resource="Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">isAlive</rdfs:label>
    </owl:DatatypeProperty>
    
	 
    <!-- http://www.thodoris.net/ontology/film-festival-model/openingHours -->

     <owl:DatatypeProperty rdf:about="openingHours">
         <rdfs:domain rdf:resource="LocalBusiness"/>
         <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">openingHours</rdfs:label>
     </owl:DatatypeProperty>
    
    
    
    <!-- http://www.thodoris.net/ontology/film-festival-model/url -->

    <owl:DatatypeProperty rdf:about="url">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">URL of the item.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">url</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
	
	<!-- http://www.thodoris.net/ontology/film-festival-model/Festival -->

    <owl:Class rdf:about="Festival">
        <owl:disjointWith rdf:resource="Person"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A festival.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Festival</rdfs:label>
    </owl:Class>
    


    <!-- http://www.thodoris.net/ontology/film-festival-model/FilmFestival -->

    <owl:Class rdf:about="FilmFestival">
        <rdfs:subClassOf rdf:resource="Festival"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A festival of films.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">FilmFestival</rdfs:label>
    </owl:Class>
    
    
	   <!-- http://schema.org/Festival -->

    <owl:Class rdf:about="http://schema.org/Festival">
        <owl:equivalentClass rdf:resource="Festival"/>
    </owl:Class>
    

    <!-- http://dbpedia.org/ontology/FilmFestival -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/FilmFestival">
        <owl:equivalentClass rdf:resource="FilmFestival"/>
    </owl:Class>
    

    <!-- http://dbpedia.org/ontology/Wikidata:Q11424 -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/Wikidata:Q11424">
        <owl:equivalentClass rdf:resource="Movie"/>
    </owl:Class>
    

    <!-- http://dbpedia.org/ontology/Wikidata:Q11707 -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/Wikidata:Q11707">
        <owl:equivalentClass rdf:resource="Restaurant"/>
    </owl:Class>
    

    <!-- http://purl.org/NET/c4dm/event.owl#Event -->

    <owl:Class rdf:about="http://purl.org/NET/c4dm/event.owl#Event"/>
    

    <!-- http://purl.org/NET/c4dm/timeline.owl#Interval -->

    <owl:Class rdf:about="http://purl.org/NET/c4dm/timeline.owl#Interval"/>
    
	
	<!-- http://www.thodoris.net/ontology/film-festival-model/Movie -->

    <owl:Class rdf:about="Movie">
        <owl:disjointWith rdf:resource="Person"/>
		<owl:disjointWith rdf:resource="Place"/>
    </owl:Class>
  
  
    <!-- http://schema.org/Movie -->

    <owl:Class rdf:about="http://schema.org/Movie">
        <owl:equivalentClass rdf:resource="Movie"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="hasDirector"/>
				<owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
    </owl:Class>
       
    <owl:Class rdf:about="Genre">
        <owl:oneOf rdf:parseType="Collection">
			<owl:Thing rdf:about="genre_Drama"/>
			<owl:Thing rdf:about="genre_Thriller"/>
			<owl:Thing rdf:about="genre_Comedy"/>
			<owl:Thing rdf:about="genre_Action"/>
			<owl:Thing rdf:about="genre_Western"/>
	  </owl:oneOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A movie genre.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Genre</rdfs:label>
    </owl:Class>


    <!-- http://schema.org/Restaurant -->

    <owl:Class rdf:about="http://schema.org/Restaurant">
        <owl:equivalentClass rdf:resource="Restaurant"/>
    </owl:Class>
    
	 <!-- http://schema.org/Cusine -->
	 <owl:Class rdf:about="Cusine">
        <owl:oneOf rdf:parseType="Collection">
			<owl:Thing rdf:about="cusine_Greek"/>
			<owl:Thing rdf:about="cusine_Italian"/>
			<owl:Thing rdf:about="cusine_Fish"/>
			<owl:Thing rdf:about="cusine_Local"/>
			<owl:Thing rdf:about="cusine_East"/>
	  </owl:oneOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cusine </rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cusine</rdfs:label>
    </owl:Class>

      

    <!-- http://www.thodoris.net/ontology/film-festival-model/Actor -->

    <owl:Class rdf:about="Actor">
        <rdfs:subClassOf rdf:resource="Person"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A movie actor.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Actor</rdfs:label>
    </owl:Class>
    


    <!-- http://www.thodoris.net/ontology/film-festival-model/Director -->

    <owl:Class rdf:about="Director">
        <rdfs:subClassOf rdf:resource="Person"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A movie director.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Director</rdfs:label>
    </owl:Class>
    
	<!-- http://www.thodoris.net/ontology/film-festival-model/AwardedDirectors -->

    <owl:Class rdf:about="AwardedDirectors">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="Director"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="academyAwards"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
		This is an Intersection of Director Class with a 
		restriction on academyAwards property to distinct actors with at least one academy award.
		</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">AwardedDirectors</rdfs:label>
    </owl:Class>
	

   
    <!-- http://www.thodoris.net/ontology/film-festival-model/LocalBusiness -->

    <owl:Class rdf:about="LocalBusiness">
        <rdfs:subClassOf rdf:resource="Place"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A particular physical business or branch of an organization. Examples of LocalBusiness include a restaurant, a particular branch of a restaurant chain, a branch of a bank, a medical practice, a club, a bowling alley, etc.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">LocalBusiness</rdfs:label>
        <skos:closeMatch rdf:resource="http://www.w3.org/ns/regorg#RegisteredOrganization"/>
    </owl:Class>
    


    <!-- http://www.thodoris.net/ontology/film-festival-model/MenActors -->

    <owl:Class rdf:about="MenActors">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="Actor"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="sex"/>
                        <owl:hasValue rdf:resource="Male"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
		This is an Intersection of Actor Class with a 
		restriction on Sex property to distinct men actors.
		</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MenActors</rdfs:label>
    </owl:Class>
    
	   <!-- http://www.thodoris.net/ontology/film-festival-model/WomenActors -->

    <owl:Class rdf:about="WomenActors">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="Actor"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="sex"/>
                        <owl:hasValue rdf:resource="Female"/>
